{
  "extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "always"],
    "@typescript-eslint/no-unused-vars": "warn",
    "arrow-body-style": "warn",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off"
  }
}
